    "@mui/material": "^5.14.4",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "tss-react": "^4.8.8"
  },
  "devDependencies": {
    "@types/node": "20.1.2",
    "@types/react": "18.2.0",
    "@types/react-dom": "18.2.7",
